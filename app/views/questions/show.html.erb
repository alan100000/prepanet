<p id="notice"><%= notice %></p>

<p>
  <b>Content:</b>
  <%= @question.content %>
</p>

<p>
  <b>Correct answer:</b>
  <%= @question.correct_answer %>
</p>

<ol>
<% for answer in @question.answers %>
  <li>
    <%=h answer.content %>
  </li>
<% end %>
</ol>


<%= link_to 'Edit', edit_question_path(@question) %> |
<%= link_to 'Back', questions_path %>
